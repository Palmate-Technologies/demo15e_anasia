<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_create_internal_transfer_form" model="ir.ui.view">
        <field name="name">view.create.internaltransfer.form</field>
        <field name="model">create.internal.transfer</field>
        <field name="arch" type="xml">
            <form string="Internal Transfer">
                <group>
                    <group>
                        <field name="name" invisible="1"/>
                        <field name="picking_type_id"/>
                        <field name="location_id"/>
                    </group>
                    <group>
                        <field name="user_id"/>
                        <field name="location_dest_id"/>
                    </group>
                </group>

                <footer>
                    <button name="process" string="Apply" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
</odoo>
